<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean class="com.smalaca.spring.ioc.service.BeansService">
        <constructor-arg>
            <bean class="com.smalaca.spring.ioc.persistance.inmemory.InMemoryBeanRepository">
                <constructor-arg>
                    <bean class="com.smalaca.spring.ioc.persistance.inmemory.UniqueIdentifierFactory"/>
                </constructor-arg>
            </bean>
        </constructor-arg>
    </bean>

</beans>